{
  "name": "yamada-docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "start": "next start",
    "prebuild": "pnpm gen:changelog && pnpm gen:search && pnpm gen:props && pnpm gen:css && pnpm gen:contributors && pnpm gen:theme && pnpm gen:content",
    "build": "next build",
    "build:profile": "next build --profile",
    "gen:changelog": "tsx scripts/changelog",
    "gen:theme": "tsx scripts/theme-component",
    "gen:css": "tsx scripts/style-props",
    "gen:props": "tsx scripts/component-props",
    "gen:search": "tsx scripts/search-content",
    "gen:contributors": "tsx scripts/contributors",
    "gen:content": "contentlayer build --clearCache",
    "gen:translate": "tsx scripts/translate-content",
    "gen:tokens": "yamada-cli tokens theme/index.ts",
    "observe:issue": "tsx scripts/observe-issues",
    "observe:pr": "tsx scripts/observe-pull-requests",
    "format": "prettier --write .",
    "format:check": "prettier --check --cache .",
    "format:write": "prettier --write --cache .",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf .contentlayer .next",
    "clean:all": "rimraf .contentlayer .next node_modules"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/core": "11.0.0",
    "@emotion/react": "11.11.0",
    "@emotion/weak-memoize": "^0.3.1",
    "@faker-js/faker": "^8.4.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@vercel/analytics": "^1.2.2",
    "@vercel/speed-insights": "^1.0.10",
    "@yamada-ui/calendar": "1.2.11",
    "@yamada-ui/carousel": "1.0.26",
    "@yamada-ui/charts": "1.1.0",
    "@yamada-ui/dropzone": "1.0.26",
    "@yamada-ui/fontawesome": "1.0.23",
    "@yamada-ui/markdown": "1.0.24",
    "@yamada-ui/nextjs": "5.0.23",
    "@yamada-ui/react": "1.3.7",
    "@yamada-ui/table": "1.0.28",
    "contentlayer": "^0.3.4",
    "dayjs": "^1.11.10",
    "glob": "^10.3.12",
    "match-sorter": "^6.3.4",
    "next": "^14.2.1",
    "next-seo": "^6.5.0",
    "prism-react-renderer": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "react-icons": "^5.0.1",
    "react-live": "^4.1.6",
    "scroll-into-view-if-needed": "^3.1.0"
  },
  "devDependencies": {
    "@clack/prompts": "^0.7.0",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@octokit/rest": "^20.1.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@yamada-ui/cli": "1.0.5",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "markdown-toc": "^1.2.0",
    "markdown-wasm": "^1.2.0",
    "next-contentlayer": "^0.3.4",
    "openai": "^4.33.0",
    "plop": "^4.0.1",
    "prettier": "^3.2.5",
    "remark-breaks": "^4.0.0",
    "remark-emoji": "4.0.0",
    "remark-gfm": "^3.0.1",
    "remark-slug": "^7.0.1",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "unified": "^10.1.2",
    "unist-util-visit": "^5.0.0"
